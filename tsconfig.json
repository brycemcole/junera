{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "src/**/*.tsx"
, "src/components/job-postings-chart.js", "src/app/register/page.js", "src/app/job-postings/saved-searches/page.js", "src/components/edit-profile.js", "src/components/ui/dropdown-menu.tsx", "src/app/job-postings/saved-searches/saved-search-form.jsx", "src/components/navbar-menu.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}